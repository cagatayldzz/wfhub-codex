{
  "$schema": "https://turbo.build/schema.v2.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["!dist/**"]
    },
    "lint": {
      "cache": true
    },
    "format": {
      "cache": false
    },
    "prettier": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
