    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^5.4.0",
    "eslint-plugin-prettier": "^5.5.5",
    "npm-check-updates": "^19.3.1",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "turbo": "^2.7.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
