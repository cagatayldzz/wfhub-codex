{
  "name": "wfhub-codex",
  "version": "0.0.1",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsc && bun run start",
    "start": "node dist/main.js",
    "build": "npm run clean && tsc && node dist/main.js",
    "clean": "rimraf dist data",
    "lint": "eslint ./src --fix && tsc --noEmit",
    "prettier": "prettier --write \"**/*.{ts,json}\" --cache",
    "format": "turbo prettier lint"
  },
  "dependencies": {
    "warframe-items": "^1.1267"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-perfectionist": "^4.10.0",
    "eslint-plugin-prettier": "^5.2.3",
    "rimraf": "^6.0.1",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "packageManager": "bun@1.2.4"
}
