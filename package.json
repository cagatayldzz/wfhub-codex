    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-check-updates": "^19.1.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "packageManager": "bun@1.3.2"
